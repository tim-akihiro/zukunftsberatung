            "<script>\n    (function(){\n      const KEY = 'zb_consent_v1';\n      const DEF = { necessary:true, analytics:false, ts:null };\n\n      const $ = (s)=>document.querySelector(s);\n      const modal   = $('#zb-consent-modal');\n      const overlay = $('#zb-consent-overlay');\n      const cbAnal  = $('#opt-analytics');\n\n      function load(){ try{ return { ...DEF, ...JSON.parse(localStorage.getItem(KEY)||'{}') }; }catch(e){ return { ...DEF }; } }\n      function save(c){ c.ts = new Date().toISOString(); localStorage.setItem(KEY, JSON.stringify(c)); }\n      function apply(consent){\n        if (typeof gtag === 'function') {\n          gtag('consent','update',{ analytics_storage: consent.analytics ? 'granted' : 'denied' });\n        } else {\n          let tries = 0; const t = setInterval(()=>{\n            if (typeof gtag === 'function' || ++tries > 20){ clearInterval(t); if (typeof gtag === 'function') gtag('consent','update',{ analytics_storage: consent.analytics ? 'granted' : 'denied' }); }\n          },150);\n        }\n      }\n      function open(){\n        const c = load();\n        if (cbAnal) cbAnal.checked = !!c.analytics;\n        overlay.style.display = 'block';\n        modal.style.display   = 'block';\n        document.documentElement.classList.add('zb-noscroll');\n      }\n      function close(){\n        overlay.style.display = 'none';\n        modal.style.display   = 'none';\n        document.documentElement.classList.remove('zb-noscroll');\n      }\n      window.ZBConsent = { open, reset: ()=>{ localStorage.removeItem(KEY); open(); } };\n\n      $('#zb-accept')?.addEventListener('click', ()=>{ const nc = { necessary:true, analytics:true };  save(nc); apply(nc); close(); });\n      $('#zb-reject')?.addEventListener('click', ()=>{ const nc = { necessary:true, analytics:false }; save(nc); apply(nc); close(); });\n      $('#zb-save')  ?.addEventListener('click', ()=>{ const nc = { necessary:true, analytics: !!cbAnal?.checked }; save(nc); apply(nc); close(); });\n      $('#zb-consent-close')?.addEventListener('click', close);\n      overlay.addEventListener('click', close);\n      $('#zb-consent-manage')?.addEventListener('click', (e)=>{ e.preventDefault(); open(); });\n\n      document.addEventListener('DOMContentLoaded', ()=>{\n        const c = load();\n        apply(c);\n        if (!c.ts) open();\n      });\n    })();\n  </script>"
          ]
