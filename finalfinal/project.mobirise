          "_customHTML": "<section data-bs-version=\"5.1\" class=\"pricing01 supportm5\" group=\"Pricing\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"12\" step=\"1\" value=\"4\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"12\" step=\"1\" value=\"3\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Label\" name=\"showLabel\">\n        <input type=\"color\" title=\"Label BG\" value=\"#f5f5f5\" name=\"labelBg\" condition=\"showLabel\">\n        <input type=\"color\" title=\"Label Border\" value=\"#ffffff\" name=\"labelBorder\" condition=\"showLabel\">\n        <input type=\"color\" title=\"Label Shadow\" value=\"#9c9c9c\" name=\"labelShadow\" condition=\"showLabel\">\n        <input type=\"checkbox\" title=\"Icon\" name=\"showIcon\" condition=\"showLabel\">\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"color\" title=\"Title Color\" value=\"#153a57\" name=\"titleColor\" condition=\"showTitle\">\n        <input type=\"color\" title=\"Title Shadow\" value=\"#ebfff5\" name=\"titleShadow\" condition=\"showTitle\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\">\n        <header>Cards</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showCardTitle\" checked>\n        <input type=\"checkbox\" title=\"Price\" name=\"showPrice\" checked>\n        <input type=\"checkbox\" title=\"Text\" name=\"showCardText\" checked>\n        <input type=\"checkbox\" title=\"Buttons\" name=\"showButtons\" checked>\n        <input type=\"checkbox\" title=\"Middle Border\" name=\"showMiddle\" checked>\n        <input type=\"color\" title=\"Middle Border Color\" value=\"#e0e0ff\" name=\"middleBorder\" condition=\"showMiddle\">\n        <input type=\"checkbox\" title=\"List\" name=\"showList\" checked>\n        <input type=\"color\" title=\"Marker Color\" value=\"#515151\" name=\"marker\" condition=\"showList\">\n        <input type=\"color\" title=\"Border\" value=\"#ffffff\" name=\"cardBorder\">\n        <input type=\"color\" title=\"BG\" value=\"#f5f5f5\" name=\"cardBg\">\n        <input type=\"color\" title=\"Shadow\" value=\"#0e2a40\" name=\"cardShadow\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"https://e.mobirise.com/supportm5/components/_images/background1.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#f4f4f4\" selected>\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"https://e.mobirise.com/supportm5/components/_images/background1.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#f5f5f5\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"col-12\">\n                <div class=\"content-wrapper\" mbr-if=\"showLabel || show